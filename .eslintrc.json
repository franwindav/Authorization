{
   "env": {
      "browser": true,
      "es6": true
   },
   "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended"],
   "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
   },
   "parser": "babel-eslint",
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true
      },
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "codeFrame": true
   },
   "plugins": ["react", "prettier", "jsx-a11y"],
   "rules": {
      "react/prop-types": 0,
      "no-undef": 0
   }
}
